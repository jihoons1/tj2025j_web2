<?xml version="1.0" encoding="UTF-8" ?>

<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="org.example.실습.실습5.XmlMapper">

    <select id="printstock" resultType="org.example.실습.실습5.BookDto">
        SELECT *
        FROM books
        WHERE title LIKE CONCAT('%', #{title}, '%')
        AND stock >= #{stock}
        ORDER BY stock ASC;
    </select>




</mapper>